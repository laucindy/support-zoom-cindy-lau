{
  "files.associations": {
    "*.scss.liquid": "scss",
    "*.js.liquid": "javascript",
    "iostream": "cpp",
    "istream": "cpp",
    "ostream": "cpp",
    "iomanip": "cpp",
    "string": "cpp"
